<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>octa_ros</name>
  <version>1.0.0</version>
  <description>OCTA-OCE ROS2 Driver</description>
  <maintainer email="rjbaw@uw.edu">Ren</maintainer>
  <license>nil</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <depend>rclcpp</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>moveit_ros_planning_interface</depend>
  <depend>moveit_ros_planning</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_eigen</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>ament_index_cpp</depend>
  <depend>rclcpp_action</depend>
  <depend>action_msgs</depend>
  <depend>builtin_interfaces</depend>
  <depend>unique_identifier_msgs</depend>
  <depend>ur_dashboard_msgs</depend>
  <depend>controller_manager_msgs</depend>
  <depend>std_srvs</depend>
  <depend>open3d</depend>
  <depend>eigen3</depend>

  <build_depend>rosidl_default_generators</build_depend>
  <exec_depend>rosidl_default_runtime</exec_depend>
  <member_of_group>rosidl_interface_packages</member_of_group>

  <build_depend>opencv4</build_depend>
  <exec_depend>opencv4</exec_depend>

  <build_depend>ament_cmake</build_depend>
  <exec_depend>ament_cmake</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>


